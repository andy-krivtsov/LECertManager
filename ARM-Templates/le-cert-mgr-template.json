                    "type": "sourcecontrols",
                    "apiVersion": "2018-11-01",
                    "location": "[resourceGroup().location]",
                    "name": "web",
                    "dependsOn": [
                        "[resourceId('Microsoft.Web/sites', variables('funcAppName'))]",
